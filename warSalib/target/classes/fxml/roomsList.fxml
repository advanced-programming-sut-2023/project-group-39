<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<Pane fx:id="roomsPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.RoomsList">
   <children>
      <Label layoutX="285.0" layoutY="40.0" text="room">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField layoutX="31.0" layoutY="98.0" prefHeight="26.0" prefWidth="254.0" fx:id="message"/>
      <Button layoutX="305.0" layoutY="99.0" mnemonicParsing="false" text="send" onMouseClicked="#send"/>
      <Button layoutX="370.0" layoutY="100.0" mnemonicParsing="false" text="edit" onMouseClicked="#edit"/>
      <Button layoutX="439.0" layoutY="101.0" mnemonicParsing="false" text="delete" onMouseClicked="#delete"/>
      <TextField layoutX="31.0" layoutY="143.0" />
      <Button layoutX="222.0" layoutY="144.0" mnemonicParsing="false" text="new chat" />
      <ScrollPane layoutX="-1.0" layoutY="191.0" prefHeight="209.0" prefWidth="600.0">
         <content>
            <VBox prefHeight="200.0" prefWidth="588.0" fx:id="messages"/>
         </content>
      </ScrollPane>
      <Button layoutX="437.0" layoutY="39.0" mnemonicParsing="false" text="back" onMouseClicked="#back"/>
   </children>
</Pane>
